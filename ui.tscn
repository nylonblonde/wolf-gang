func _ready():
	var popup_menu = get_popup()
	
	popup_menu.connect(\"index_pressed\", self, \"item_handler\")

func item_handler(id):
	match id:
		0: #new
			print(\"new\")
		1: #open
			print(\"open\")
		2: #save
			print(\"save\")
		3: #save as
			print(\"save as\")

